{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2019",
    "allowJs": false,
    "strict": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "outDir": "dist",
    "baseUrl": "./",
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "include": [
    "./src",
    "../common/src",
    "../../testUtils",
    "babel.config.js",
    "jest.config.js",
    "webpack.*.config.js",
    "run.ts",
    "./tests",
    "fixtures",
    "__mocks__",
    "testSetupFile.ts"
  ]
}
